<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StudentInformationPage">

    <View
        android:id="@+id/divider6"
        android:layout_width="409dp"
        android:layout_height="330dp"
        android:background="#B3DCC8"
        app:layout_constraintBottom_toTopOf="@+id/divider9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/divider9"
        android:layout_width="409dp"
        android:layout_height="400dp"
        android:layout_marginBottom="4dp"
        android:background="#D3D2D2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/divider12"
        android:layout_width="390dp"
        android:layout_height="200dp"
        android:background="#9CC0AF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.387" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/user"
        app:layout_constraintBottom_toTopOf="@+id/divider12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/divider6"
        app:layout_constraintVertical_bias="0.309"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/button"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@android:drawable/ic_popup_reminder"
        app:layout_constraintBottom_toTopOf="@+id/divider12"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.197"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.363"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/backMenu"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="24dp"
        android:background="@android:drawable/ic_menu_my_calendar"
        android:onClick="backMenu"
        app:layout_constraintBottom_toTopOf="@+id/divider12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.804"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.237"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/noteInformationBtn"
        android:layout_width="110dp"
        android:layout_height="75dp"
        android:background="@android:drawable/ic_menu_sort_by_size"
        android:fontFamily="sans-serif"
        android:gravity="bottom|center"
        android:onClick="loginPanel"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/divider12"
        app:layout_constraintEnd_toEndOf="@+id/divider12"
        app:layout_constraintHorizontal_bias="0.021"
        app:layout_constraintStart_toStartOf="@+id/divider12"
        app:layout_constraintTop_toTopOf="@+id/divider12"
        app:layout_constraintVertical_bias="0.044"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/absenceInformationBtn"
        android:layout_width="110dp"
        android:layout_height="75dp"
        android:layout_marginEnd="72dp"
        android:background="@android:drawable/ic_menu_agenda"
        android:fontFamily="sans-serif"
        android:gravity="bottom|center"
        android:onClick="loginPanel"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/divider12"
        app:layout_constraintEnd_toEndOf="@+id/divider12"
        app:layout_constraintHorizontal_bias="0.26"
        app:layout_constraintStart_toEndOf="@+id/noteInformationBtn"
        app:layout_constraintTop_toTopOf="@+id/divider12"
        app:layout_constraintVertical_bias="0.037"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/courseProgramBtn"
        android:layout_width="110dp"
        android:layout_height="75dp"
        android:background="@android:drawable/ic_menu_today"
        android:fontFamily="sans-serif"
        android:gravity="bottom|center"
        android:onClick="loginPanel"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/divider12"
        app:layout_constraintEnd_toEndOf="@+id/divider12"
        app:layout_constraintHorizontal_bias="0.833"
        app:layout_constraintStart_toEndOf="@+id/absenceInformationBtn"
        app:layout_constraintTop_toTopOf="@+id/divider12"
        app:layout_constraintVertical_bias="0.037"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/examDatesBtn"
        android:layout_width="110dp"
        android:layout_height="75dp"
        android:background="@android:drawable/ic_menu_month"
        android:fontFamily="sans-serif"
        android:gravity="bottom|center"
        android:onClick="loginPanel"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/divider12"
        app:layout_constraintEnd_toEndOf="@+id/divider12"
        app:layout_constraintHorizontal_bias="0.021"
        app:layout_constraintStart_toStartOf="@+id/divider12"
        app:layout_constraintTop_toBottomOf="@+id/noteInformationBtn"
        app:layout_constraintVertical_bias="0.873"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/writtenAveragesBtn"
        android:layout_width="110dp"
        android:layout_height="75dp"
        android:background="@android:drawable/ic_menu_sort_alphabetically"
        android:fontFamily="sans-serif"
        android:gravity="bottom|center"
        android:onClick="loginPanel"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/divider12"
        app:layout_constraintEnd_toEndOf="@+id/divider12"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintStart_toEndOf="@+id/examDatesBtn"
        app:layout_constraintTop_toBottomOf="@+id/absenceInformationBtn"
        app:layout_constraintVertical_bias="0.927"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/yearNotesBtn"
        android:layout_width="110dp"
        android:layout_height="75dp"
        android:background="@android:drawable/ic_input_get"
        android:fontFamily="sans-serif"
        android:gravity="bottom|center"
        android:onClick="loginPanel"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/divider12"
        app:layout_constraintEnd_toEndOf="@+id/divider12"
        app:layout_constraintHorizontal_bias="0.833"
        app:layout_constraintStart_toEndOf="@+id/writtenAveragesBtn"
        app:layout_constraintTop_toBottomOf="@+id/courseProgramBtn"
        app:layout_constraintVertical_bias="0.915"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/nameLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Efe Hasan GÃ¼ltekin"
        app:layout_constraintBottom_toTopOf="@+id/divider12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="@+id/divider6"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.162" />

    <TextView
        android:id="@+id/numLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="11/U 6226"
        app:layout_constraintBottom_toTopOf="@+id/divider12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/divider6"
        app:layout_constraintTop_toBottomOf="@+id/nameLabel" />

    <ListView
        android:id="@+id/ListView1"
        android:layout_width="409dp"
        android:layout_height="310dp"
        app:layout_constraintBottom_toBottomOf="@+id/divider9"
        app:layout_constraintEnd_toEndOf="@+id/divider9"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/divider9"
        app:layout_constraintTop_toBottomOf="@+id/divider12"
        app:layout_constraintVertical_bias="0.14" />

</androidx.constraintlayout.widget.ConstraintLayout>